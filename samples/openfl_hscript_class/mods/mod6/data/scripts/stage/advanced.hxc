import Stage;
import openfl.Assets;

class AdvancedStage extends Stage {
  public function new() {
    super("advanced");
    this.stageName = "Advanced Stage";
  }

  public override function create():Void {
    super.create();

    var landscapeBg = new Bitmap(Assets.getBitmapData('img/stage/landscape.png'));
    landscapeBg.x = 0;
    landscapeBg.y = 0;
    landscapeBg.width = 480;
    landscapeBg.width = 360;

    this.addChild(landscapeBg);
  }
}